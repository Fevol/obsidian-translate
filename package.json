{
	"name": "obsidian-translate",
	"version": "1.4.7",
	"description": "Translate text and notes in Obsidian.md with Google Translate, DeepL, Azure, and more.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"build:dev": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs development",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "jest --verbose",
		"test-coverage": "jest --coverage"
	},
	"keywords": [],
	"author": "Fevol",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^29.5.8",
		"@types/obsidian-typings": "npm:obsidian-typings@^1.1.5",
		"esbuild-jest": "^0.5.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jest-fetch-mock": "^3.0.3",
		"jsdom": "^22.1.0",
		"ts-jest": "^29.1.1"
	},
	"dependencies": {
		"@tsconfig/svelte": "^5.0.2",
		"@types/node": "^20.9.0",
		"@typescript-eslint/eslint-plugin": "^6.10.0",
		"@typescript-eslint/parser": "^6.10.0",
		"builtin-modules": "^3.3.0",
		"esbuild": "^0.19.5",
		"esbuild-plugin-inline-worker": "github:mitschabaude/esbuild-plugin-inline-worker",
		"esbuild-sass-plugin": "^2.16.0",
		"esbuild-svelte": "^0.8.0",
		"install": "^0.13.0",
		"iso-639-1": "^3.1.0",
		"jsnpm": "^0.4.1",
		"monkey-around": "^2.3.0",
		"obsidian": "^1.4.11",
		"svelte": "^4.2.3",
		"svelte-dnd-action": "^0.9.31",
		"svelte-preprocess": "^5.1.0",
		"tslib": "2.6.2",
		"typescript": "^5.2.2"
	}
}
