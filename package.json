		"@typescript-eslint/eslint-plugin": "^5.27.1",
		"@typescript-eslint/parser": "^5.27.1",
		"builtin-modules": "^3.2.0",
		"esbuild": "0.13.12",
		"esbuild-sass-plugin": "^2.2.6",
		"esbuild-svelte": "^0.7.1",
		"install": "^0.13.0",
		"jest": "^29.0.1",
		"jest-environment-jsdom": "^29.0.1",
		"jsdom": "^19.0.0",
		"jsnpm": "^0.4.1",
		"obsidian": "latest",
		"svelte": "^3.49.0",
